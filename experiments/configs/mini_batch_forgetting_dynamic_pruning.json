{
  "experiment_name": "mini_batch_forgetting_dynamic_pruning",
  "description": "Mini-batch forgetting with dynamic pruning techniques",
  "dataset_name": "cifar10",
  "data_dir": "./data",
  "model_name": "google/vit-base-patch16-224",
  "use_vit": true,
  "num_classes": 10,
  "batch_size": 23,
  "learning_rate": 0.0001,
  "device": "cuda:3",
  "unlearning_strategy": "dynamic_pruning",
  "forget_ratio": 0.1,
  "test_ratio": 0.2,
  "training_epochs": 2,
  "pruning_ratio": 0.05,
  "importance_threshold": 0.5,
  "fine_tune_epochs": 3,
  "forget_loss_weight": 0.2,
  "prune_classifier_only": true,
  "wandb_project": "unlearning-experiments",
  "results_dir": "./results"
}

